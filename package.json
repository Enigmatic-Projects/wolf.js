{
    "name": "repo-template",
    "version": "1.0.0-alpha.1",
    "description": "repo-template description",
    "keywords": [],
    "files": [
        "dist/",
        "wrapper.mjs",
        "!**/*.tsbuildinfo"
    ],
    "directories": {
        "doc": "docs/",
        "example": "examples/",
        "lib": "lib/",
        "test": "tests/"
    },
    "type": "commonjs",
    "main": "./dist/index.js",
    "exports": {
        "import": "./wrapper.mjs",
        "require": "./dist/index.js"
    },
    "types": "./dist/index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/Enigmatic-Projects/repo-template"
    },
    "scripts": {
        "compile": "rimraf ./dist && tsc",
        "test": "npm run format:check && npm run compile && nyc mocha --require ts-node/register --reporter spec --slow 200 --bail --timeout 1000 tests/repo-template.ts",
        "format:check": "prettier --check \"lib/**/*.ts\" \"tests/**/*.ts\"",
        "format:fix": "prettier --write \"lib/**/*.ts\" \"tests/**/*.ts\""
    },
    "contributors": [
        {
            "name": "Repo Template",
            "email": "info@ecproj.io"
        }
    ],
    "engines": {
        "node": ">=10.0.0"
    },
    "devDependencies": {
        "@types/mocha": "^8.2.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.17.0",
        "eslint-config-prettier": "^7.1.0",
        "expect.js": "^0.3.1",
        "mocha": "^8.2.1",
        "nyc": "^15.1.0",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@types/node": "^14.14.20"
    }
}
